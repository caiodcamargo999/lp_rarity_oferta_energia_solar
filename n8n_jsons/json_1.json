{
  "name": "ElevenLabs Agent Example",
  "nodes": [
    {
      "parameters": {
        "httpMethod": "POST",
        "path": "elevenlabs-agent",
        "options": {}
      },
      "id": "1",
      "name": "Webhook Trigger",
      "type": "n8n-nodes-base.webhook",
      "typeVersion": 1,
      "position": [250, 300]
    },
    {
      "parameters": {
        "url": "https://api.elevenlabs.io/v1/agents/{{agent_id}}/interact",
        "method": "POST",
        "authentication": "headerAuth",
        "jsonParameters": true,
        "options": {},
        "bodyParametersJson": "={\n  \"input\": \"{{$json[\"query\"]}}\"\n}"
      },
      "id": "2",
      "name": "Send to ElevenLabs Agent",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [500, 300],
      "credentials": {
        "httpHeaderAuth": {
          "id": "elevenlabs-credentials",
          "name": "ElevenLabs API"
        }
      }
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={\"reply\": $json[\"data\"][\"output\"]}"
      },
      "id": "3",
      "name": "Return Response",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [750, 300]
    }
  ],
  "connections": {
    "Webhook Trigger": {
      "main": [
        [
          {
            "node": "Send to ElevenLabs Agent",
            "type": "main",
            "index": 0
          }
        ]
      ]
    },
    "Send to ElevenLabs Agent": {
      "main": [
        [
          {
            "node": "Return Response",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  ],
  "credentials": {
    "httpHeaderAuth": {
      "elevenlabs-credentials": {
        "user": "api-key",
        "password": "{{ELEVENLABS_API_KEY}}"
      }
    }
  }
}
